  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rimraf dist",
    "compile": "pnpm run clean && node scripts/updateSentryKeys.js && electron-webpack",
    "compile:local": "pnpm run clean && electron-webpack",
    "compileCheck": "tsc --project tsconfig.json",
    "createEnvFile": "node scripts/createEnvFile.js",
    "create-a-release": "node scripts/release-automation/create-a-release.js",
    "create-candidate": "node scripts/release-automation/create-candidate.js",
    "dato-sync": "node scripts/datoSync.js",
    "dev": "pnpm run clean && cross-env NODE_CONFIG_ENV=development electron-webpack dev",
    "deploy-release-build": "node scripts/release-automation/deploy-release-build.js",
    "deploy-release-yaml": "node scripts/release-automation/deploy-release-yaml.js",
    "deploy-update-rollout": "node scripts/release-automation/deploy-update-rollout.js",
    "deploy-rollback": "node scripts/release-automation/deploy-rollback.js",
    "dist": "pnpm run compile && node scripts/dist.js",
    "dist:local": "pnpm run compile:local && node scripts/dist.js",
    "dist:dir": "pnpm run dist -- --dir -c.compression=store -c.mac.identity=null",
    "dist:repackage-with-publish-url": "node scripts/repackage.js",
    "dist:version-update-files": "node scripts/versionUpdateFiles.js",
    "generate-icons": "svgr --icon --ignore-existing --replace-attr-values \"#000=currentColor\" -d assets/generated/ assets/originals/",
    "generate-typecheck": "npx typia generate --input src/renderer/typecheck --output src/renderer/typecheck/generated --project tsconfig.json",
    "generate-gpus": "ts-node scripts/generateGpus.ts",
    "watch-typecheck": "node scripts/watchTypeCheck.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src/",
    "locize:copy-version": "locize copy-version",
    "locize:sync": "locize sync --path ./static/translations --reference-language-only false",
    "migrate-create": "migrate create --template-file ./migration-template.ts",
    "prettier": "pnpm run clean && prettier \"**/*.ts\" \"**/*.js\" \"**/*.json\" --check",
    "release:dry-run": "pnpm run compileCheck",
    "rust-cam-version-check": "node scripts/checkRustCamVersion.js",
    "test": "rimraf output && jest --env=jsdom",
    "test:integration": "jest --config=jest.integration.config.js --runInBand --forceExit",
    "test:integration-ci": "pnpm run createEnvFile && jest --config=jest.integration-ci.config.js --runInBand --forceExit",
    "test:record-core": "jest --config=jest.record-core.config.js --runInBand --forceExit",
    "test:ci": "rimraf output && jest --env=jsdom --config=jest.ci.config.js --coverage --ci",
    "test:watch": "jest --env=jsdom --watch",
    "test:jest-helpers": "jest --env=jsdom --config=jest.helpers.config.js scripts/jest/helpers",
    "update-migration-map": "node scripts/release-automation/update-migration-map.js",
    "update-prerelease": "node scripts/updatePrerelease.js",
    "update-fixversions": "node scripts/updateFixVersions.js"
  },
